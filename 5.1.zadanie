from tkinter import *

window = Tk()
frames = {}
ents = {}
labels = ["имя", "фамилию", "отчество"]

for i in range(1, 3 + 1):

    def read():
        print(ents[i].get())
        ents[i].delete(0, END)

    frames[i] = Frame(window)
    frames[i].pack()
    
    Label(frames[i], text=f"Введите {labels[i-1]}").pack()
    
    ents[i] = Entry(frames[i], width=20)
    ents[i].pack()
    
    Button(frames[i], text="Ввести", command=read).pack()


window.mainloop()