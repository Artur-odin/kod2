def sum_list(lst):
    summ = 0
    for x in lst:
        if type(x) == int:
            summ += x
        else:
            summ += sum_list(x)
    return summ

lst = [1, 2, 3, [1, [1, 2, 5], 1, 1, 1], 2, 3, 5]
print(sum_list(lst))

def count_paths(x, y):
    if x == y:
        return 1
    if x > y:
        return 0
    else:
        return count_paths(x+1, y) + count_paths(x+3, y)

    
result = count_paths(1, 9) * count_paths(9, 17)
print(f"Количество программ: {result}")